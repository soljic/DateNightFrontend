{
  "compilerOptions": {
    "target": "es6",
    "jsx": "preserve",
    "baseUrl": ".",
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "@/components/*": ["./components/*"],
      "@/styles/*": ["./styles/*"],
      "@/service/*": ["./service/*"],
      "@/utils/*": ["./utils/*"],
      "@/hooks/*": ["./hooks/*"]
    }
  },
  "exclude": ["node_modules"]
}
